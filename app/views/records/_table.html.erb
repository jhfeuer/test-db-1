<table class="display">


  <tbody>
    
    <tr>
      <th></th>
      <th>Serial Number</th>
      <th>Product</th>
      <th>Part Number</th>
      <th>PW QN</th>
      <th>Removal Date</th>
      <th>Supplier</th>
      <th>Status</th>
    </tr>
    
    <% @records.each do |record| %>
      <tr>
        <td><%= link_to 'Show', record %></td>
        <td><%= link_to record.serialNum, edit_record_path(record) %></td>
        <td><%= record.product %></td>
        <td><%= record.partNum %></td>
        <td><%= record.qn %></td>
        <td><%= record.removalDate %></td>
        <td><%= record.supplier %></td>
        <td><%= record.status %></td>
      </tr>
    <% end %>
  </tbody>
</table>
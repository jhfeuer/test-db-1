<%= form_tag(search_path, method: "get") do %>
<div>
  <%= label_tag(:snQuery, "Serial Number:") %>
  <%= text_field_tag(:snQuery) %>
  <%= label_tag(:productQuery, "Product:") %>
  <%= text_field_tag(:productQuery) %>
  <%= label_tag(:supplierQuery, "Supplier:") %>
  <%= text_field_tag(:supplierQuery) %>
  <%= submit_tag("Search", :name => nil) %>
</div>

<%= render 'table' %>
<% end %>